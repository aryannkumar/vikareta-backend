{
  "dashboard": {
    "id": null,
    "title": "Vikareta Business Metrics",
    "tags": [
      "vikareta",
      "business"
    ],
    "panels": [
      {
        "id": 1,
        "title": "New Users",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(vikareta_new_users_total[1h])",
            "legendFormat": "New Users/Hour"
          }
        ]
      },
      {
        "id": 2,
        "title": "Active RFQs",
        "type": "stat",
        "targets": [
          {
            "expr": "vikareta_active_rfqs_total",
            "legendFormat": "Active RFQs"
          }
        ]
      },
      {
        "id": 3,
        "title": "Order Conversion Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "(vikareta_orders_total / vikareta_rfqs_total) * 100",
            "legendFormat": "Conversion %"
          }
        ]
      },
      {
        "id": 4,
        "title": "Revenue Trend",
        "type": "graph",
        "targets": [
          {
            "expr": "increase(vikareta_revenue_total[1h])",
            "legendFormat": "Revenue/Hour"
          }
        ]
      }
    ]
  }
}